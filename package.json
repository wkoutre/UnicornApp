{
  "name": "UnicornApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "kw": "watchman watch-del-all",
    "kn": "killall -9 node",
    "nukeYarn": "rm -f yarn.lock && yarn",
    "lint": "tslint 'src/**/*.{ts,tsx}'",
    "start": "node node_modules/react-native/local-cli/cli.js start --reset-cache --port",
    "killwatch": "watchman watch-del-all",
    "killnode": "killall -9 node",
    "test": "jest",
    "reverse": "adb reverse tcp:8081 tcp:8081",
    "cleanAndroid": "./android/gradlew clean -p ./android/ && rm -rf android/build/ && rm -rf android/app/build/",
    "cleanIos": "rm -rf ios/build/ && rm -rf ~/Library/Developer/Xcode/DerivedData/*",
    "cleantp": "rm -rf node_modules/react-native/third-party/",
    "postInstall": "rndebugger-open"
  },
  "dependencies": {
    "react": "16.6.1",
    "react-native": "0.57.7",
    "react-native-animatable": "^1.3.0",
    "react-native-audio-toolkit": "^1.0.6",
    "react-native-common-lib": "^1.6.12",
    "react-native-elements": "^0.19.1",
    "react-native-keyboard-aware-scroll-view": ">= 0.4.4",
    "react-native-keyboard-spacer": "^0.4.1",
    "react-native-modal": ">= 5.2.0",
    "react-native-switch": ">= 1.5.0",
    "react-native-track-player": "https://github.com/react-native-kit/react-native-track-player.git#dev",
    "react-navigation": "^2.14.2",
    "react-navigation-redux-helpers": "^2.0.5",
    "react-redux": "^5.1.1",
    "redux": "^4.0.0",
    "redux-enhancer-react-native-appstate": "^0.3.1",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "typesafe-actions": "^2.0.4",
    "underscore": "^1.9.1",
    "validator": "^10.9.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "@types/node": "^10.12.8",
    "@types/react": "^16.7.5",
    "@types/react-native": "^0.57.8",
    "@types/react-navigation": "^2.13.1",
    "@types/react-test-renderer": "^16.0.2",
    "babel-eslint": "^10.0.1",
    "babel-jest": "23.6.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-native": "^4.0.1",
    "colors": "^1.3.2",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-import-resolver-typescript": "^1.0.2",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-native": "^3.3.0",
    "jest": "23.6.0",
    "metro-react-native-babel-preset": "0.50.0",
    "node-fetch": "^2.3.0",
    "prettier": "^1.15.2",
    "prompts": "^1.1.0",
    "prop-types": "^15.6.2",
    "react-native-typescript-transformer": "^1.2.10",
    "react-test-renderer": "16.6.1",
    "ts-jest": "^23.1.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.16.0",
    "tslint-react": "^3.6.0",
    "tslint-react-recommended": "^1.0.15",
    "typescript": "^3.0.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+.(js)$": "<rootDir>/node_modules/babel-jest",
      ".(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|.(test|spec)).(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      ".snap$",
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest/cache"
  }
}
